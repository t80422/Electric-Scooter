@model string
@{
    Layout = null;
}
<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Print - Customer List</title>
</head>
<body>
    <iframe id="pdf-frame" src="data:application/pdf;base64,@Model" style="width: 100%; height: 100vh;" frameborder="0"></iframe>
    <script>
        window.onload = function () {
            var iframe = document.getElementById('pdf-frame').contentWindow;
            iframe.focus();
            iframe.print();
        };
    </script>
</body>
</html>
